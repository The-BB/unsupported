     __name__ = "SevenZip"
     __type__ = "extractor"
-    __version__ = "0.38"
+    __version__ = "0.39"
     __status__ = "testing"
 
     __description__ = """7-Zip extractor plugin"""
@@ -74,7 +74,7 @@ class SevenZip(BaseExtractor):
     _RE_FILES = re.compile(
         r"([\d\-]+)\s+([\d:]+)\s+([RHSA.]+)\s+(\d+)\s+(?:(\d+)\s+)?(.+)"
