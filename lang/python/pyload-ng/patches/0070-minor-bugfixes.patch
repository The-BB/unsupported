Subject: [PATCH 70/73] minor bugfixes

---
 src/pyload/core/__init__.py                | 2 +-
 src/pyload/core/threads/download_thread.py | 2 +-
 src/pyload/webui/webserver_thread.py       | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/src/pyload/core/__init__.py b/src/pyload/core/__init__.py
index 45ef1fc7b..d7d556c89 100644
--- a/src/pyload/core/__init__.py
+++ b/src/pyload/core/__init__.py
@@ -379,7 +379,7 @@ class Core:
             self.log.info(self._("Cache directory: {}").format(self.tempdir))
 
             storage_folder = self.config.get("general", "storage_folder")
-            self.log.info(self._("Storage directory: {}".format(storage_folder)))
+            self.log.info(self._("Storage directory: {}").format(storage_folder))
 
             avail_space = format.size(fs.free_space(storage_folder))
             self.log.info(self._("Storage free space: {}").format(avail_space))
diff --git a/src/pyload/core/threads/download_thread.py b/src/pyload/core/threads/download_thread.py
index 4402bdb8a..1819db1e0 100644
--- a/src/pyload/core/threads/download_thread.py
+++ b/src/pyload/core/threads/download_thread.py
@@ -49,7 +49,7 @@ class DownloadThread(PluginThread):
                 # this pyfile was deleted while queueing
 
                 pyfile.plugin.check_for_same_files(starting=True)
-                self.pyload.log.info(self._("Download starts: {}".format(pyfile.name)))
+                self.pyload.log.info(self._("Download starts: {}").format(pyfile.name))
 
                 # start download
                 self.pyload.addon_manager.download_preparing(pyfile)
diff --git a/src/pyload/webui/webserver_thread.py b/src/pyload/webui/webserver_thread.py
index 7570321d6..c6922fb21 100644
--- a/src/pyload/webui/webserver_thread.py
+++ b/src/pyload/webui/webserver_thread.py
@@ -65,7 +65,7 @@ class WebServerThread(threading.Thread):
             # TODO: Not implemented
 
     def run(self):
-        self.log.warning(
+        self.log.info(
             self._("Starting webserver: {scheme}://{host}:{port}").format(
                 scheme="https" if self.use_ssl else "http",
                 host=f"[{self.host}]" if ":" in self.host else self.host,
